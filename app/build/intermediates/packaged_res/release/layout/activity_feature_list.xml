<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Blue_3"
    tools:context=".feature_list" >

    <ImageView
        android:id="@+id/bubleImage"
        android:layout_width="410dp"
        android:layout_height="280dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/circle_white_top"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="0dp" />

    <ImageView
        android:id="@+id/userImage"
        android:layout_width="98dp"
        android:layout_height="98dp"
        android:layout_marginTop="40dp"
        android:background="@drawable/circle_green"
        android:backgroundTint="@color/white"
        android:padding="7dp"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/bubleImage"
        app:srcCompat="@drawable/person"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="Hello"
        android:textColor="#ffffff"
        android:textSize="22sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/userImage"
        app:layout_constraintStart_toStartOf="@+id/userImage"
        app:layout_constraintTop_toBottomOf="@+id/userImage" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="Welcome Back !"
        android:textColor="@color/ic_launcher_background"
        android:textSize="36sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/textView2"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/bottom_background_light_green"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3">

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="20sp"
                android:gravity="center">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="16dp"
                    android:orientation="vertical">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/white_btn_background">

                        <ImageView
                            android:id="@+id/iV_text_to_QR"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_margin="8dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="1.0"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="1.0"
                            app:srcCompat="@drawable/circle_green"
                            app:tint='@color/Blue_1' />

                        <ImageView
                            android:id="@+id/iV_logo_text_to_QR"
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            app:layout_constraintBottom_toBottomOf="@+id/iV_text_to_QR"
                            app:layout_constraintEnd_toEndOf="@+id/iV_text_to_QR"
                            app:layout_constraintStart_toStartOf="@+id/iV_text_to_QR"
                            app:layout_constraintTop_toTopOf="@+id/iV_text_to_QR"

                            app:srcCompat="@drawable/outline_document_scanner_24"
                            app:tint="@color/white" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <TextView
                        android:id="@+id/text_text_to_QR"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Text to QR"
                        android:textAlignment="center"
                        android:textColor="@color/black"
                        android:textSize="11sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="16dp"
                    android:orientation="vertical">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/white_btn_background">

                        <ImageView
                            android:id="@+id/iV_url_to_QR"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_margin="8dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.0"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:tint='@color/Blue_1'
                            app:srcCompat="@drawable/circle_green" />

                        <ImageView
                            android:id="@+id/iV_logo_url_to_QR"
                            app:layout_constraintBottom_toBottomOf="@+id/iV_url_to_QR"
                            app:layout_constraintEnd_toEndOf="@+id/iV_url_to_QR"
                            app:layout_constraintStart_toStartOf="@+id/iV_url_to_QR"
                            app:layout_constraintTop_toTopOf="@+id/iV_url_to_QR"
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            app:srcCompat="@drawable/outline_laptop_mac_24"
                            app:tint="@color/white" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <TextView
                        android:id="@+id/text_url_to_QR"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="URL to QR"
                        android:textAlignment="center"
                        android:textColor="@color/black"
                        android:textSize="11sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="16dp"
                    android:orientation="vertical">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/white_btn_background">

                        <ImageView
                            android:id="@+id/iV_msg_to_QR"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_margin="8dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.0"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"

                            app:tint='@color/Blue_1'
                            app:srcCompat="@drawable/circle_green" />

                        <ImageView
                            android:id="@+id/iV_logo_msg_to_QR"
                            app:layout_constraintBottom_toBottomOf="@+id/iV_msg_to_QR"
                            app:layout_constraintEnd_toEndOf="@+id/iV_msg_to_QR"
                            app:layout_constraintStart_toStartOf="@+id/iV_msg_to_QR"
                            app:layout_constraintTop_toTopOf="@+id/iV_msg_to_QR"
                            app:srcCompat="@drawable/outline_message_24"
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            app:tint="@color/white" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <TextView
                        android:id="@+id/text_msg_to_QR"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Message to QR"
                        android:textAlignment="center"
                        android:textColor="@color/black"
                        android:textSize="11sp" />
                </LinearLayout>

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="16dp"
                    android:orientation="vertical">

                    <androidx.constraintlayout.widget.ConstraintLayout

                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/white_btn_background">

                        <ImageView
                            android:id="@+id/iV_call_to_QR"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_margin="8dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.0"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:tint='@color/Blue_1'
                            app:srcCompat="@drawable/circle_green" />

                        <ImageView
                            android:id="@+id/iV_logo_call_to_QR"
                            app:layout_constraintBottom_toBottomOf="@+id/iV_call_to_QR"
                            app:layout_constraintEnd_toEndOf="@+id/iV_call_to_QR"
                            app:layout_constraintStart_toStartOf="@+id/iV_call_to_QR"
                            app:layout_constraintTop_toTopOf="@+id/iV_call_to_QR"
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            app:srcCompat="@drawable/outline_wifi_calling_24"
                            app:tint="@color/white" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <TextView
                        android:id="@+id/text_call_to_QR"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Call to QR"
                        android:textAlignment="center"
                        android:textColor="@color/black"
                        android:textSize="11sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="16dp"
                    android:orientation="vertical">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/white_btn_background">

                        <ImageView
                            android:id="@+id/iV_wts_to_QR"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_margin="8dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.0"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"

                            app:tint='@color/Blue_1'
                            app:srcCompat="@drawable/circle_green" />

                        <ImageView
                            android:id="@+id/iV_logo_wts_to_QR"
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            app:layout_constraintBottom_toBottomOf="@+id/iV_wts_to_QR"
                            app:layout_constraintEnd_toEndOf="@+id/iV_wts_to_QR"
                            app:layout_constraintStart_toStartOf="@+id/iV_wts_to_QR"
                            app:layout_constraintTop_toTopOf="@+id/iV_wts_to_QR"
                            app:srcCompat="@drawable/icons8_whatsapp"
                            app:tint="@color/white"
                            android:contentDescription="Whatsapp Icons" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <TextView
                        android:id="@+id/text_wts_to_QR"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Whatsapp to QR"
                        android:textAlignment="center"
                        android:textColor="@color/black"
                        android:textSize="11sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="16dp"
                    android:orientation="vertical">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/white_btn_background">

                        <ImageView
                            android:id="@+id/iV_upi_to_QR"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_margin="8dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.0"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:tint='@color/Blue_1'
                            app:srcCompat="@drawable/circle_green" />

                        <ImageView
                            android:id="@+id/iV_logo_upi_to_QR"
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            app:layout_constraintBottom_toBottomOf="@+id/iV_upi_to_QR"
                            app:layout_constraintEnd_toEndOf="@+id/iV_upi_to_QR"
                            app:layout_constraintStart_toStartOf="@+id/iV_upi_to_QR"
                            app:layout_constraintTop_toTopOf="@+id/iV_upi_to_QR"
                            app:srcCompat="@drawable/outline_account_balance_wallet_24"
                            app:tint="@color/white" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <TextView
                        android:id="@+id/text_upi_to_QR"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="UPI to QR"
                        android:textAlignment="center"
                        android:textColor="@color/black"
                        android:textSize="11sp" />
                </LinearLayout>

            </TableRow>

        </TableLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>